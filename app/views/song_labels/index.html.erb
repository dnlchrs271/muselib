<p id="notice"><%= notice %></p>

<h1>Listing Song Labels</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @song_labels.each do |song_label| %>
      <tr>
        <td><%= song_label.title %></td>
        <td><%= link_to 'Show', song_label %></td>
        <td><%= link_to 'Edit', edit_song_label_path(song_label) %></td>
        <td><%= link_to 'Destroy', song_label, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song label', new_genre_playlist_song_label_path(genre_id: @genre.id, playlist_id: @playlist.id, id: @song_label.id)  %>
